<% @coverage_hash.each do |key, value| %>
	<h3>Reports for <%=key%></h3>
	<ul>
		<% value.each do |report| %>
			<% sha = report.first 
			   build_id = report.last	
			%>
			<li>Coverage report for: <a href="coverage_view/<%=sha%>/index.html"><%=sha[0..6]%></a>, Link to <a href="https://magnum.travis-ci.com/<%=key%>/builds/<%=build_id%>">Travis build</a></li>
		<%end%>
	</ul>
<% end %>